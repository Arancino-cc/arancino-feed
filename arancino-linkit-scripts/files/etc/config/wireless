config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11n'
        option path 'platform/10300000.wmac'
        option htmode 'HT20'
        option txpower '20'
        option country 'IT'
        option disabled '0'

config wifi-iface
        option device 'radio0'
        option mode 'ap'
        option network 'lan'
        option encryption 'none'
        option ssid 'SmartME'

